<UserControl x:Class="Indulged.Plugins.Dashboard.SummersaltRenderers.SummersaltContactPhotoFooterRenderer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" >
        <Button Foreground="#f28422" FontSize="28" FontFamily="/Assets/Fonts/BenchNine-Regular.ttf#BenchNine"
                Content="Show All Contacts" HorizontalAlignment="Center" 
                Margin="0, 0, 15, 0" 
                Style="{StaticResource BorderlessButtonStyle}" />

    </Grid>
</UserControl>